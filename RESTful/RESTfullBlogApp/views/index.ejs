<%- include("partials/header"); %>


<h1>
	Index Page!!
</h1>

<% blogs.forEach(function(blog){ %>
    <h1>
		<%= blog.title %>
</h1>
<img src="<%= blog.image %>">
<h4>
	<%= blog.date %>
</h4>
<p>
	<%= blog.body.substring(0,100) %>...
</p>
<a href="/blogs/<%= blog._id %>">Read More</a>
<% }) %>

<%- include("partials/footer"); %>
